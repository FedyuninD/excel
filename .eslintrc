{
  "parser":"babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "linebreak-style": "off",
    "indent": "off",
    "comma-dangle": "off",
    "semi": "off",
    "arrow-parens": "off",
    "key-spacing": "off",
    "require-jsdoc": "off",
    "object-curly-spacing": "off",
    "no-multi-spaces": "off",
    "eol-last": "off",
    "quotes": "off",
    "no-trailing-spaces": "off",
    "max-len": "off",
    "no-invalid-this": "off",
    "operator-linebreak": "off"

  },
  "extends": ["eslint:recommended", "google"]
}